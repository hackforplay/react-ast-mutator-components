{
  "name": "@hackforplay/react-ast-mutator-components",
  "version": "1.2.0",
  "description": "React components to mutate Babel AST.",
  "main": "lib/index.js",
  "directories": {
    "lib": "lib",
    "src": "src"
  },
  "files": [
    "lib",
    "src"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hackforplay/react-ast-mutator-components"
  },
  "scripts": {
    "test": "npm run lint && npm run build && ava --fail-fast -s",
    "watch": "ava --fail-fast -s -w",
    "build": "tsc --build",
    "lint": "prettier 'src/**/*' -l",
    "format": "prettier 'src/**/*' --write",
    "start": "parcel ./public/index.html",
    "fixtures": "cp -r test/fixtures/ dist/",
    "deploy": "npm run fixtures && npx parcel build public/index.html --public-url './' && npx gh-pages -d dist",
    "semantic-release": "semantic-release"
  },
  "author": "teramotodaiki",
  "license": "MIT",
  "ava": {
    "files": [
      "src/**/*.test.ts"
    ],
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "devDependencies": {
    "@types/enzyme": "^3.9.1",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/node": "^11.13.2",
    "ava": "^1.4.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "gh-pages": "^2.0.1",
    "parcel-bundler": "^1.12.3",
    "prettier": "^1.16.4",
    "semantic-release": "^15.13.19",
    "ts-node": "^8.0.3",
    "typescript": "^3.4.3"
  },
  "dependencies": {
    "@babel/parser": "^7.4.3",
    "@babel/traverse": "^7.4.3",
    "@babel/types": "^7.4.0",
    "@types/babel__traverse": "^7.0.6",
    "@types/react": "^16.8.13",
    "@types/react-dom": "^16.8.3",
    "@types/redux": "^3.6.0",
    "immer": "^3.1.3",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "redux": "^4.0.2",
    "rxjs": "^6.5.2",
    "typescript-fsa": "^3.0.0-beta-2"
  }
}
