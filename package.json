{
  "name": "@hackforplay/react-ast-mutator-components",
  "version": "1.0.0",
  "description": "React components to mutate Babel AST.",
  "main": "lib/index.js",
  "scripts": {
    "test": "npm run lint && npm run build && ava",
    "build": "tsc",
    "lint": "prettier 'src/**/*' -l",
    "format": "prettier 'src/**/*' --write",
    "serve": "parcel ./public/index.html",
    "semantic-release": "semantic-release"
  },
  "author": "teramotodaiki",
  "license": "MIT",
  "ava": {
    "files": [
      "src/**/*.test.ts"
    ],
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "devDependencies": {
    "@types/node": "^11.13.2",
    "ava": "^1.4.1",
    "parcel-bundler": "^1.12.3",
    "prettier": "^1.16.4",
    "semantic-release": "^15.13.3",
    "ts-node": "^8.0.3",
    "typescript": "^3.4.3"
  }
}
